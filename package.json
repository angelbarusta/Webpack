{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "curso webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js --mode development",
    "build:multi": "webpack --config ./multi-entry-points/webpack.config.js --mode development",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js --mode development",
    "build:plugins": "webpack --config ./plugins/webpack.config.js --mode development",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --mode development ",
    "build:babel": "webpack-dev-server --config ./Babel/webpack.config.js --mode development ",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js --mode development ",
    "build:hook": "webpack-dev-server --config ./react-hook/webpack.config.js --mode development ",
    "build:url": "webpack-dev-server --config ./url-loader/webpack.config.js --mode development ",
    "build:prepro": "webpack-dev-server --config ./prepro/webpack.config.js --mode development "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angelbarusta/Webpack.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@angelbarusta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angelbarusta/Webpack/issues"
  },
  "homepage": "https://github.com/angelbarusta/Webpack#readme",
  "dependencies": {
    "@babel/runtime": "7.4.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "css-loader": "2.1.1",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "less": "^3.11.1",
    "less-loader": "5.0.0",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "^1.1.3",
    "stylus": "^0.54.7",
    "stylus-loader": "3.0.2",
    "url-loader": "1.1.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "3.5.1"
  }
